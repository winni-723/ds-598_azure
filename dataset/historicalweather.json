{
	"name": "historicalweather",
	"properties": {
		"linkedServiceName": {
			"referenceName": "HttpServer2_historicalweather",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"lat": {
				"type": "string",
				"defaultValue": "42.3601"
			},
			"lon": {
				"type": "string",
				"defaultValue": "-71.0589"
			},
			"start": {
				"type": "string",
				"defaultValue": "div(sub(ticks(addDays(utcNow(), -7 * weeksAgo)), ticks('1970-01-01T00:00:00Z')), 10000000)"
			},
			"end": {
				"type": "string",
				"defaultValue": "div(sub(ticks(addDays(utcNow(), -7 * weeksAgo + 7)), ticks('1970-01-01T00:00:00Z')), 10000000)"
			},
			"appid": {
				"type": "string",
				"defaultValue": "469a42d10f53a9d772f37cb07942e59d"
			}
		},
		"annotations": [],
		"type": "Json",
		"typeProperties": {
			"location": {
				"type": "HttpServerLocation",
				"relativeUrl": "data/2.5/history/city?lat=@{dataset().lat}&lon=@{dataset().lon}&type=@{dataset().dataType}&start=@{dataset().start}&end=@{dataset().end}&appid=@{dataset().appid}"
			}
		},
		"schema": {}
	}
}